#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int n,i;
    scanf("%d",&n);
    int ar[n];
    for(i=1;i<=n;i++)
    {
    scanf("%d",&ar[i]);
    }
    for(i=n;i>=1;i--) 
    if(i%2==0)
    {
    printf("%d ",ar[i]);
    }
    return 0;   
    return 0;
}
